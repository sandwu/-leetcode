

class Node:
    def __init__(self, val):
        self.val = val
        self.next  = None


a = Node(10)
b = Node(11)
c = Node(12)
d = Node(13)
e = Node(14)
f = Node(15)

a.next = b
b.next = c
c.next = d
d.next = e
e.next = f


def traversal(head):
    """traversal linked listï¼Œ so can get work"""
    res = [head.val]
    while head.next:
        res.append(head.next.val)
        head = head.next
    return res


print(traversal(a))


def reverse(head):
    """reverse linked list"""
    prev = None
    while head:
        curr = head
        head = head.next
        curr.next = prev
        prev = curr
    return prev


print(traversal(reverse(a)))
